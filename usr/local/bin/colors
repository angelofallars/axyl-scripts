#!/bin/env bash
# Description: Metadata for dmenu stuff

# dmenu colors
if [[ "$DESKTOP_SESSION" == "Qtile" ]]; then
  bg="#1d1f21"
  altbg="#666666"
  fg="#c5c8c6"
elif [[ "$DESKTOP_SESSION" == "dwm" ]]; then
  bg="#1E222A"
  altbg="#5C6370"
  fg="#ABB2BF"
elif [[ "$DESKTOP_SESSION" == "bspwm" ]]; then
  bg="#282a36"
  altbg="#555555"
  fg="#f8f8f2"
elif [[ "$DESKTOP_SESSION" == "Xmonad" ]]; then
  bg="#2E3440"
  altbg="#4C566A"
  fg="#D8DEE9"
fi
